{
  "name": "resolve-cloud-common",
  "version": "0.0.137",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "scripts": {
    "prepare": "node scripts/prepare.js",
    "clear": "node scripts/clear.js",
    "lint": "eslint --ignore-path=.prettierignore \"./**/*.{ts,tsx}\" && tsc --noEmit",
    "prettier": "prettier --write \"**/**.{ts,tsx,json}\"",
    "prettier:scripts": "prettier --write \"**/scripts/**.js\"",
    "test": "yarn clear && jest",
    "test:f": "echo || exit 0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "uuid": "^8.3.1",
    "node-rsa": "1.1.1",
    "netmask": "2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.17",
    "@types/debug": "^4.1.5",
    "@types/uuid": "^8.3.0",
    "@types/node-rsa": "1.0.0",
    "@types/netmask": "1.0.30",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "aws-sdk": "2.890.0",
    "jest": "^26.4.2",
    "lodash": "^4.17.15",
    "prettier": "^2.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "lint-staged": "^10.2.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.5"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.unit.test.ts?(x)",
      "**/?(*.)+(spec|test).unit.test.ts?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  }
}
