{
  "name": "resolve-cloud-common",
  "version": "0.0.24",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "scripts": {
    "prepare": "node scripts/prepare.js",
    "clear": "node scripts/clear.js",
    "lint": "eslint --ignore-path=.prettierignore './**/*.{ts,tsx}' && tsc --noEmit",
    "prettier": "prettier --write \"**/**.{ts,tsx,json}\"",
    "prettier:scripts": "prettier --write \"**/scripts/**.js\"",
    "test": "yarn clear && jest",
    "test:f": "echo || exit 0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "lodash.isequal":"^4.5.0",
    "lodash.differencewith": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "aws-sdk": "2.585.0",
    "jest": "^25.5.4",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "lint-staged": "^10.2.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.unit.test.ts?(x)",
      "**/?(*.)+(spec|test).unit.test.ts?(x)"
    ],
    "roots": [
      "<rootDir>"
    ]
  }
}
